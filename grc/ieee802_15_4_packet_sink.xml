<?xml version="1.0"?>
<block>
	<name>Packet Sink</name>
	<key>ieee802_15_4_packet_sink</key>
	<category>IEEE802.15.4</category>
	<import>import ieee802_15_4</import>
	<make>ieee802_15_4.packet_sink($th)</make>

	<param>
		<name>Threshold</name>
		<key>th</key>
		<value>10</value>
		<type>int</type>
	</param>

	<sink>
		<name>in</name>
		<type>float</type>
		<nports>1</nports>
	</sink>
	<source>
		<name>out</name>
		<type>message</type>
	</source>
</block>
